#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n;
    cin>>n;
    char c;
    int count=0;
    int count2=0;
    for(int i=0;i<n;i++)
    {
        cin>>c;
        if(c=='z')
        {
            count++;
        }
        if(c=='n')
        {
            count2++;
        }
    }
    while(count2--)
    {
        cout<<1<<' ';
    }
    while(count--)
    {
        cout<<0<<' ';
    }
    
    

    return 0;
}
